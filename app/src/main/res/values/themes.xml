<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MobilePOSe" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->

        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="Theme.MobilePOSe" parent="Base.Theme.MobilePOSe" />

    <style name="Theme.MobilePOSe.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MobilePOSe.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MobilePOSe.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="BottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/BottomSheetStyle</item>
    </style>

    <style name="BottomSheetStyle" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="CustomSearchBar" parent="Widget.Material3.SearchBar">
        <item name="android:background">@color/material_dynamic_neutral99</item>
    </style>







</resources>